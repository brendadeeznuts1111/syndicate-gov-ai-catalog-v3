{
  "name": "@syndicate/gov-rules",
  "version": "3.0.3",
  "description": "Enterprise governance rules engine with AI-enhanced validation, quantum-safe compliance, and real-time policy enforcement",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./validators": {
      "import": "./dist/validators/index.js",
      "require": "./dist/validators/index.cjs",
      "types": "./dist/validators/index.d.ts"
    },
    "./parsers": {
      "import": "./dist/parsers/index.js",
      "require": "./dist/parsers/index.cjs",
      "types": "./dist/parsers/index.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance/index.js",
      "require": "./dist/compliance/index.cjs",
      "types": "./dist/compliance/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "build:cjs": "bun build src/index.ts --outdir dist --target node --format cjs --outfile index.cjs",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "bun tsc --noEmit",
    "typecheck": "bun tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build && bun run build:cjs",
    "validate": "bun run src/cli/validate.ts",
    "analyze": "bun install --analyze",
    "audit": "bun audit --severity=high",
    "audit:json": "bun audit --json > audit-report.json",
    "governance:check": "bun run src/scripts/governance-check.ts",
    "compliance:verify": "bun run src/scripts/compliance-verify.ts"
  },
  "dependencies": {
    "express": "5.1.0",
    "react": "19.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "bun-types": "^1.3.0",
    "@types/uuid": "^11.0.0"
  },
  "bin": {
    "syndicate-gov": "./dist/cli/index.js",
    "gov-validate": "./dist/cli/validate.js"
  },
  "keywords": [
    "governance",
    "rules",
    "compliance",
    "validation",
    "policy",
    "enterprise",
    "ai-enhanced",
    "quantum-safe",
    "syndicate",
    "governance-engine",
    "policy-enforcement",
    "regulatory",
    "audit",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syndicate/ai-catalog.git",
    "directory": "packages/gov-rules"
  },
  "bugs": {
    "url": "https://github.com/syndicate/ai-catalog/issues"
  },
  "homepage": "https://github.com/syndicate/ai-catalog/tree/main/packages/gov-rules#readme",
  "author": {
    "name": "Syndicate Governance Team",
    "email": "governance@syndicate.com",
    "url": "https://syndicate.com/governance"
  },
  "maintainers": [
    {
      "name": "Syndicate Governance Team",
      "email": "governance@syndicate.com",
      "url": "https://syndicate.com/governance"
    },
    {
      "name": "Compliance Office",
      "email": "compliance@syndicate.com",
      "url": "https://syndicate.com/compliance"
    }
  ],
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=20.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/syndicate"
  },
  "config": {
    "enterprise": true,
    "quantum_safe": true,
    "ai_enhanced": true,
    "performance_tier": "supreme",
    "governance_enabled": true
  },
  "syndicate": {
    "category": "governance",
    "tier": "enterprise",
    "features": [
      "ai-enhanced-validation",
      "quantum-safe-compliance",
      "real-time-policy-enforcement",
      "governance-engine",
      "regulatory-audit",
      "blockchain-verification"
    ],
    "performance": {
      "target_validation_time": "50ms",
      "target_compliance_check": "100ms",
      "concurrent_rules": 1e5
    },
    "compliance": [
      "SOC2",
      "ISO27001",
      "GDPR",
      "HIPAA",
      "PCI-DSS",
      "SOX"
    ],
    "governance": {
      "policy_engine": "ai-enhanced",
      "validation_framework": "quantum-safe",
      "audit_trail": "blockchain-verified",
      "enforcement_mode": "real-time"
    }
  }
}