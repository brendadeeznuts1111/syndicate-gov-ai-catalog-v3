{
  "name": "@syndicate/dashboard",
  "version": "3.0.3",
  "description": "Enterprise-grade business intelligence dashboard with real-time analytics, KPI monitoring, and holographic visualization capabilities",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target browser --format esm",
    "build:cjs": "bun build src/index.ts --outdir dist --target browser --format cjs --outfile index.cjs",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "bun tsc --noEmit",
    "typecheck": "bun tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build && bun run build:cjs",
    "analyze": "bun install --analyze",
    "audit": "bun audit --severity=high",
    "audit:json": "bun audit --json > audit-report.json"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "4.1.16"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "typescript": "^5.0.4",
    "bun-types": "^1.3.0"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "keywords": [
    "dashboard",
    "analytics",
    "business-intelligence",
    "kpi",
    "monitoring",
    "visualization",
    "enterprise",
    "react",
    "typescript",
    "syndicate",
    "real-time",
    "holographic",
    "synesthetic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syndicate/ai-catalog.git",
    "directory": "packages/dashboard"
  },
  "bugs": {
    "url": "https://github.com/syndicate/ai-catalog/issues"
  },
  "homepage": "https://github.com/syndicate/ai-catalog/tree/main/packages/dashboard#readme",
  "author": {
    "name": "Syndicate AI Team",
    "email": "ai-team@syndicate.com",
    "url": "https://syndicate.com"
  },
  "maintainers": [
    {
      "name": "Syndicate AI Team",
      "email": "ai-team@syndicate.com",
      "url": "https://syndicate.com"
    },
    {
      "name": "Enterprise Support",
      "email": "enterprise@syndicate.com",
      "url": "https://syndicate.com/enterprise"
    }
  ],
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=20.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/syndicate"
  },
  "config": {
    "enterprise": true,
    "quantum_safe": true,
    "ai_enhanced": true,
    "performance_tier": "supreme"
  },
  "syndicate": {
    "category": "business-intelligence",
    "tier": "enterprise",
    "features": [
      "real-time-analytics",
      "kpi-monitoring",
      "holographic-visualization",
      "synesthetic-experience",
      "quantum-safe",
      "ai-enhanced"
    ],
    "performance": {
      "target_load_time": "100ms",
      "target_render_time": "50ms",
      "concurrent_users": 1e4
    },
    "compliance": [
      "SOC2",
      "ISO27001",
      "GDPR",
      "HIPAA"
    ]
  }
}