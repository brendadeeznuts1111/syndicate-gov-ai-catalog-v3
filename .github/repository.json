  "engines": {
    "bun": ">=1.3.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "prepare": "bun run build",
    "precommit": "bun run 🛡️ validate:enterprise && bun run pr:enforce",
    "prepush": "bun run ci:full && bun run 🧹 rituals:housekeep",
    "postinstall": "bun run 🧙 wizard:enhanced",
    "release": "bun run 🚀 rituals:complete && bun run 📊 analytics:export"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "rituals": {
      "aiEnhanced": true,
      "quantumSafe": true,
      "enterpriseMode": true,
      "visualFeedback": true,
      "predictiveMode": true,
      "complianceLevel": "soc2"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "bun run precommit",
      "pre-push": "bun run prepush",
      "post-merge": "bun run 🧹 rituals:housekeep"
    }
  },
  "enterprise": {
    "compliance": {
      "soc2": true,
      "iso27001": true,
      "auditTrail": true,
      "blockchainReady": true
    },
    "analytics": {
      "realtime": true,
      "predictive": true,
      "businessIntelligence": true,
      "exportMetrics": true
    },
    "security": {
      "quantumSafe": true,
      "postQuantumCryptography": true,
      "enhancedValidation": true
    },
    "performance": {
      "parallelProcessing": true,
      "memoryOptimization": true,
      "ultraFastExecution": true,
      "intelligentCaching": true
    }
  }
